<table id="hm" class="">
  <tr>
    <td></td>
    {{#each range as |angle|}}
      <td class="index">{{angle}}</td>
    {{/each}}
  </tr>
  {{#each table as |row|}}
    <tr>
      <td class="index index-column"></td>
      {{#each row as |data|}}
        <td>
          <div class="cell" {{action "cellClicked" data}} data-distance="{{data.[0]}}" data-color="{{data.[1]}}" data-zx="{{data.[2]}}" data-zy="{{data.[3]}}">
            {{#bs-popover triggerEvents="hover" placement="top" class="cell-popover"}}
              <table class="table">
                <tr>
                  <th>z -> x</th>
                  <td>{{data.[2]}}&deg;</td>
                </tr>
                <tr>
                  <th>z -> y</th>
                  <td>{{data.[3]}}&deg;</td>
                </tr>
                <tr>
                  <th>result</th>
                  <td>{{data.[0]}}</td>
                </tr>
              </table>
            {{/bs-popover}}
          </div>
        </td>
      {{/each}}
    </tr>
  {{/each}}
</table>